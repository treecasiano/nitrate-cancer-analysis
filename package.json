{
  "name": "vue-leaflet-express-boilerplate",
  "version": "0.0.1",
  "description": "Web app starter using Vue, Vuetify, Leaflet, and Express",
  "author": "Tree Casiano",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "integration": "jest --bail --detectOpenHandles --runInBand --verbose __tests__integration__/",
    "start": "node app.js"
  },
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "args": "^5.0.0",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "body-parser": "^1.18.3",
    "config": "^2.0.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "express-openapi": "^2.0.4",
    "got": "^9.2.1",
    "leaflet": "^1.4.0",
    "pg-promise": "^8.6.4",
    "regenerator-runtime": "^0.13.2",
    "vue": "^2.6.6",
    "vue-router": "^3.0.1",
    "vue2-leaflet": "^2.0.2",
    "vuetify": "^2.0.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.5.0",
    "@vue/cli-plugin-eslint": "^3.5.0",
    "@vue/cli-plugin-unit-jest": "^3.8.0",
    "@vue/cli-service": "^3.5.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^3.4.1",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.0.0",
    "material-design-icons-iconfont": "^5.0.1",
    "prettier": "^1.18.2",
    "sass": "^1.17.4",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-vuetify": "^0.6.3",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.2.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "license": "ISC"
}
